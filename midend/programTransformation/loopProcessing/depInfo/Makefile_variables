mptlpDepInfo_cleanLocal=\
	rm -rf \
		$(mptlpDepInfoPath)/cxx_templates \
		$(mptlpDepInfoPath)/ii_files \
		$(mptlpDepInfoPath)/ti_files \
		$(mptlpDepInfoPath)/Templates.DB

mptlpDepInfo_distCleanLocal=\
	rm -rf \
		$(mptlpDepInfoPath)/Templates.DB

