# Non-recursive Makefile

mptlpComputation_la_sources= \
	$(mptlpComputationPath)/LoopTree.C \
	$(mptlpComputationPath)/LoopTreeDummyNode.C \
	$(mptlpComputationPath)/LoopTreeShadow.C \
	$(mptlpComputationPath)/LoopTreeLocality.C \
	$(mptlpComputationPath)/LoopTreeDepComp.C \
	$(mptlpComputationPath)/DepCompTransform.C \
	$(mptlpComputationPath)/LoopTreeTransform.C

mptlpComputation_includeHeaders=\
	$(mptlpComputationPath)/LoopTreeDummyNode.h \
	$(mptlpComputationPath)/LoopTreeShadow.h \
	$(mptlpComputationPath)/LoopTreeLocality.h \
	$(mptlpComputationPath)/LoopTreeBuild.h \
	$(mptlpComputationPath)/LoopTree.h \
	$(mptlpComputationPath)/LoopTreeObserver.h \
	$(mptlpComputationPath)/LoopTreeTransform.h \
	$(mptlpComputationPath)/LoopTreeDepComp.h \
	$(mptlpComputationPath)/LoopTreeHoldNode.h \
	$(mptlpComputationPath)/DepCompTransform.h

mptlpComputation_extraDist=\
	$(mptlpComputationPath)/CMakeLists.txt

mptlpComputation_cleanLocal=\
	rm -rf \
		$(mptlpComputationPath)/cxx_templates \
		$(mptlpComputationPath)/ii_files \
		$(mptlpComputationPath)/ti_files \
		$(mptlpComputationPath)/Templates.DB

mptlpComputation_distCleanLocal=\
	rm -rf \
		$(mptlpComputationPath)/Templates.DB

