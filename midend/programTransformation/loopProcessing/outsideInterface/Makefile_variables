	rm -rf \
		$(mptlpOutsideInterfacePath)/core \
		$(mptlpOutsideInterfacePath)/ii_files \
		$(mptlpOutsideInterfacePath)/ti_files \
		$(mptlpOutsideInterfacePath)/Templates.DB

