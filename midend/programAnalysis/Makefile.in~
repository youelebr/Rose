	$(AM_V_CXXLD)$(CXXLINK) $(am_libdistributedMemoryAnalysis_la_rpath) $(libdistributedMemoryAnalysis_la_OBJECTS) $(libdistributedMemoryAnalysis_la_LIBADD) $(LIBS)

