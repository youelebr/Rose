	$(mpaValuepropagation_extraDist) \
	$(mpaVariableRenaming_extraDist)

all: $(BUILT_SOURCES)
	$(MAKE) $(AM_MAKEFLAGS) all-recursive

.SUFFIXES:
.SUFFIXES: .C .cc .cpp .lo .o .obj
$(srcdir)/Makefile.in:  $(srcdir)/Makefile.am $(top_srcdir)/config/Makefile.for.ROSE.includes.and.libs $(srcdir)/annotationLanguageParser/Makefile_variables $(srcdir)/CallGraphAnalysis/Makefile_variables $(srcdir)/CFG/Makefile_variables $(srcdir)/bitvectorDataflow/Makefile_variables $(srcdir)/VirtualFunctionAnalysis/Makefile_variables $(srcdir)/defUseAnalysis/Makefile_variables $(srcdir)/distributedMemoryAnalysis/Makefile_variables $(srcdir)/dominanceAnalysis/Makefile_variables $(srcdir)/genericDataflow/Makefile_variables $(srcdir)/graphAnalysis/Makefile_variables $(srcdir)/OAWrap/Makefile_variables $(srcdir)/pointerAnal/Makefile_variables $(srcdir)/sideEffectAnalysis/Makefile_variables $(srcdir)/staticInterproceduralSlicing/Makefile_variables $(srcdir)/valuePropagation/Makefile_variables $(srcdir)/variableRenaming/Makefile_variables $(am__configure_deps)
	@for dep in $?; do \
	  case '$(am__configure_deps)' in \
	    *$$dep*) \
	      ( cd $(top_builddir) && $(MAKE) $(AM_MAKEFLAGS) am--refresh ) \
	        && { if test -f $@; then exit 0; else break; fi; }; \
	      exit 1;; \
	  esac; \
	done; \
	echo ' cd $(top_srcdir) && $(AUTOMAKE) --foreign --ignore-deps src/midend/programAnalysis/Makefile'; \
	$(am__cd) $(top_srcdir) && \
	  $(AUTOMAKE) --foreign --ignore-deps src/midend/programAnalysis/Makefile
.PRECIOUS: Makefile
Makefile: $(srcdir)/Makefile.in $(top_builddir)/config.status
	@case '$?' in \
	  *config.status*) \
	    cd $(top_builddir) && $(MAKE) $(AM_MAKEFLAGS) am--refresh;; \
	  *) \
	    echo ' cd $(top_builddir) && $(SHELL) ./config.status $(subdir)/$@ $(am__depfiles_maybe)'; \
	    cd $(top_builddir) && $(SHELL) ./config.status $(subdir)/$@ $(am__depfiles_maybe);; \
	esac;
$(top_srcdir)/config/Makefile.for.ROSE.includes.and.libs $(srcdir)/annotationLanguageParser/Makefile_variables $(srcdir)/CallGraphAnalysis/Makefile_variables $(srcdir)/CFG/Makefile_variables $(srcdir)/bitvectorDataflow/Makefile_variables $(srcdir)/VirtualFunctionAnalysis/Makefile_variables $(srcdir)/defUseAnalysis/Makefile_variables $(srcdir)/distributedMemoryAnalysis/Makefile_variables $(srcdir)/dominanceAnalysis/Makefile_variables $(srcdir)/genericDataflow/Makefile_variables $(srcdir)/graphAnalysis/Makefile_variables $(srcdir)/OAWrap/Makefile_variables $(srcdir)/pointerAnal/Makefile_variables $(srcdir)/sideEffectAnalysis/Makefile_variables $(srcdir)/staticInterproceduralSlicing/Makefile_variables $(srcdir)/valuePropagation/Makefile_variables $(srcdir)/variableRenaming/Makefile_variables:

$(top_builddir)/config.status: $(top_srcdir)/configure $(CONFIG_STATUS_DEPENDENCIES)
	cd $(top_builddir) && $(MAKE) $(AM_MAKEFLAGS) am--refresh

$(top_srcdir)/configure:  $(am__configure_deps)
	cd $(top_builddir) && $(MAKE) $(AM_MAKEFLAGS) am--refresh
$(ACLOCAL_M4):  $(am__aclocal_m4_deps)
	cd $(top_builddir) && $(MAKE) $(AM_MAKEFLAGS) am--refresh
$(am__aclocal_m4_deps):

clean-noinstLTLIBRARIES:
	-test -z "$(noinst_LTLIBRARIES)" || rm -f $(noinst_LTLIBRARIES)
	@list='$(noinst_LTLIBRARIES)'; \
	locs=`for p in $$list; do echo $$p; done | \
	      sed 's|^[^/]*$$|.|; s|/[^/]*$$||; s|$$|/so_locations|' | \
	      sort -u`; \
	test -z "$$locs" || { \
	  echo rm -f $${locs}; \
	  rm -f $${locs}; \
	}

libannotationLanguageParser.la: $(libannotationLanguageParser_la_OBJECTS) $(libannotationLanguageParser_la_DEPENDENCIES) $(EXTRA_libannotationLanguageParser_la_DEPENDENCIES) 
	$(AM_V_CXXLD)$(CXXLINK)  $(libannotationLanguageParser_la_OBJECTS) $(libannotationLanguageParser_la_LIBADD) $(LIBS)

