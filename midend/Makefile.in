distdir: $(DISTFILES)
	$(mkdir_p) $(distdir)/$(mAbstractHandlePath) $(distdir)/$(mAbstractMemoryObjectPath) $(distdir)/$(mAstDiagnosticsPath) $(distdir)/$(mAstProcessingPath) $(distdir)/$(mAstQueryPath) $(distdir)/$(mAstRewriteMechanismPath) $(distdir)/$(mProgramTransformationPath) $(distdir)/$(mauAnnotationPath) $(distdir)/$(mauAstInterfacePath) $(distdir)/$(mauAstSupportPath) $(distdir)/$(mauSymbolicValPath) $(distdir)/$(mptAstInliningPath) $(distdir)/$(mptConstantFoldingPath) $(distdir)/$(mptFiniteDifferencingPath) $(distdir)/$(mptFunctionCallNormalizationPath) $(distdir)/$(mptImplicitCodeGenerationPath) $(distdir)/$(mptOmpLoweringPath) $(distdir)/$(mptOutliningPath) $(distdir)/$(mptPartialRedundancyEliminationPath) $(distdir)/../../config $(distdir)/abstractHandle $(distdir)/abstractMemoryObject $(distdir)/astDiagnostics $(distdir)/astProcessing $(distdir)/astQuery $(distdir)/astRewriteMechanism $(distdir)/astUtil $(distdir)/astUtil/annotation $(distdir)/astUtil/astInterface $(distdir)/astUtil/astSupport $(distdir)/astUtil/symbolicVal $(distdir)/programTransformation $(distdir)/programTransformation/astInlining $(distdir)/programTransformation/astOutlining $(distdir)/programTransformation/constantFolding $(distdir)/programTransformation/finiteDifferencing $(distdir)/programTransformation/functionCallNormalization $(distdir)/programTransformation/implicitCodeGeneration $(distdir)/programTransformation/ompLowering $(distdir)/programTransformation/partialRedundancyElimination
	@srcdirstrip=`echo "$(srcdir)" | sed 's|.|.|g'`; \
	topsrcdirstrip=`echo "$(top_srcdir)" | sed 's|.|.|g'`; \
	list='$(DISTFILES)'; for file in $$list; do \
	  case $$file in \
	    $(srcdir)/*) file=`echo "$$file" | sed "s|^$$srcdirstrip/||"`;; \
	    $(top_srcdir)/*) file=`echo "$$file" | sed "s|^$$topsrcdirstrip/|$(top_builddir)/|"`;; \
	  esac; \
	  if test -f $$file || test -d $$file; then d=.; else d=$(srcdir); fi; \
	  dir=`echo "$$file" | sed -e 's,/[^/]*$$,,'`; \
	  if test "$$dir" != "$$file" && test "$$dir" != "."; then \
	    dir="/$$dir"; \
	    $(mkdir_p) "$(distdir)$$dir"; \
	  else \
	    dir=''; \
	  fi; \
	  if test -d $$d/$$file; then \
	    if test -d $(srcdir)/$$file && test $$d != $(srcdir); then \
	      cp -pR $(srcdir)/$$file $(distdir)$$dir || exit 1; \
	    fi; \
	    cp -pR $$d/$$file $(distdir)$$dir || exit 1; \
	  else \
	    test -f $(distdir)/$$file \
	    || cp -p $$d/$$file $(distdir)/$$file \
	    || exit 1; \
	  fi; \
	done
	list='$(DIST_SUBDIRS)'; for subdir in $$list; do \
	  if test "$$subdir" = .; then :; else \
	    test -d "$(distdir)/$$subdir" \
	    || $(mkdir_p) "$(distdir)/$$subdir" \
	    || exit 1; \
	    distdir=`$(am__cd) $(distdir) && pwd`; \
	    top_distdir=`$(am__cd) $(top_distdir) && pwd`; \
	    (cd $$subdir && \
	      $(MAKE) $(AM_MAKEFLAGS) \
	        top_distdir="$$top_distdir" \
	        distdir="$$distdir/$$subdir" \
	        distdir) \
	      || exit 1; \
	  fi; \
	done
